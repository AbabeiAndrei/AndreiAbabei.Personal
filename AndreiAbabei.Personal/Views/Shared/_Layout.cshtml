<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/App/css")

    @Scripts.Render("~/App/Polyfill")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/angular")
    <script src="~/app/Scripts/Controllers/layoutController.js"></script>
</head>
<body ng-app="andreiApp">
    <div class="left-col" ng-controller="layoutController">
        <div class="main">
            <a href="/">
                <img src="data:image/png;base64," alt="me" class="me" />
                @*<img src="~/Resources/mebw.png" alt="me" class="me"/>*@
            </a>
            <p class="name">Andrei Ababei</p>
            <p class="motto">... complicated thinks made simple</p>
        </div>
        <ul class="menu">
            <li>
                <a href="/About" id="about_link" ng-class="{active:activePage==1}">about</a>
            </li>
            <li>
                <a href="/Posts" id="posts_link" ng-class="{active:activePage==2}">posts</a>
            </li>
            <li>
                <a href="/Contact" id="contact_link" ng-class="{active:activePage==3}">contact</a>
            </li>
        </ul>
        
        <div class="social">
            <ul class="social-icons icon-circle icon-zoom list-unstyled list-inline"> 
                <li> <a href="https://www.linkedin.com/in/ababeiandrei/" target="_blank"><i class="fa fa-linkedin"></i></a></li> 
                <li> <a href="skype:andr3y0o" target="_blank"><i class="fa fa-skype"></i></a></li>
                <li> <a href="https://stackoverflow.com/users/3092184/andrew" target="_blank"><i class="fa fa-stack-overflow"></i></a></li>
                <li> <a href="https://github.com/AbabeiAndrei" target="_blank"><i class="fa fa-github"></i></a></li>
            </ul>
        </div>
    </div>
    <div class="main-col">
        @RenderBody()
        <hr />
        <footer class="footer">
            <p>&copy; 2018 - Andrei Ababei</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", false)
</body>
</html>
